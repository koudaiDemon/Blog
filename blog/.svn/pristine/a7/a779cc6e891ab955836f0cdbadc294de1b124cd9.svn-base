public class Comm {

	private int id;
	private String content;
	private Date commTime;
	private int aid;
	private int parentcommid;
	private int uid;
	
	public Comm() {
		// TODO Auto-generated constructor stub
	}

	public Comm(int id, String content, Date commTime, int aid, int parentcommid, int uid) {
		super();
		this.id = id;
		this.content = content;
		this.commTime = commTime;
		this.aid = aid;
		this.parentcommid = parentcommid;
		this.uid = uid;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public Date getCommTime() {
		return commTime;
	}

	public void setCommTime(Date commTime) {
		this.commTime = commTime;
	}

	public int getAid() {
		return aid;
	}

	public void setAid(int aid) {
		this.aid = aid;
	}

	public int getParentcommid() {
		return parentcommid;
	}

	public void setParentcommid(int parentcommid) {
		this.parentcommid = parentcommid;
	}

	public int getUid() {
		return uid;
	}

	public void setUid(int uid) {
		this.uid = uid;
	}
	
	
	
}
