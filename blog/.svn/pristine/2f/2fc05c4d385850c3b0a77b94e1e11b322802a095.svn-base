package com.softeem.dto;

import java.util.Date;

/**
 * ”—«È¡¥Ω”DTO
 * @author lx
 *
 */
public class Link {

	private int id;
	private String name;
	private String url;
	private String linkimgSrc;
	private String information;
	private int target;
	private int rel;
	private Date addTime;
	private Date uTime;
	public Link() {
		// TODO Auto-generated constructor stub
	}

	public Link(int id, String name, String url, String linkimgSrc ,String information, int target, int rel,Date addTime,Date uTime) {
		super();
		this.id = id;
		this.name = name;
		this.url = url;
		this.linkimgSrc = linkimgSrc;
		this.information = information;
		this.target = target;
		this.rel = rel;
		this.addTime = addTime;
		this.uTime=uTime;
	}
	
	
	
	public Date getuTime() {
		return uTime;
	}

	public void setuTime(Date uTime) {
		this.uTime = uTime;
	}

	public Date getAddTime() {
		return addTime;
	}

	public void setAddTime(Date addTime) {
		this.addTime = addTime;
	}

	public String getInformation() {
		return information;
	}

	public void setInformation(String information) {
		this.information = information;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String getLinkimgSrc() {
		return linkimgSrc;
	}

	public void setLinkimgSrc(String linkimgSrc) {
		this.linkimgSrc = linkimgSrc;
	}

	public int getTarget() {
		return target;
	}

	public void setTarget(int target) {
		this.target = target;
	}

	public int getRel() {
		return rel;
	}

	public void setRel(int rel) {
		this.rel = rel;
	}
	
	
	
}
